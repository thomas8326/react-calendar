{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "camelcase": [
      "error",
      {
        "ignoreList": ["UNSAFE_componentDidMount", "_id$", "^UNSAFE_"]
      }
    ]
  }
}
